{
  "name": "ProjectName",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:clean": "cd android && ./gradlew clean && cd ..",
    "android:staging": "react-native run-android --mode=staging",
    "android:staging:install": "cd android && ./gradlew installStaging && cd ..",
    "android:staging:bundle": "cd android && ./gradlew bundleStaging && cd ..",
    "android:staging:assemble": "cd android && ./gradlew assembleStaging && cd ..",
    "android:production": "react-native run-android --mode=release",
    "android:production:install": "cd android && ./gradlew installRelease && cd ..",
    "android:production:bundle": "cd android && ./gradlew bundleRelease && cd ..",
    "android:production:assemble": "cd android && ./gradlew assembleRelease && cd ..",
    "ios": "react-native run-ios",
    "ios:clean": "xattr -w com.apple.xcode.CreatedByBuildSystem true ./ios/build && cd ios && xcodebuild clean && cd ..",
    "ios:staging": "react-native run-ios --scheme \"ProjectName-Staging\"",
    "ios:production": "react-native run-ios --scheme \"ProjectName-Production\"",
    "ios:ipad": "react-native run-ios --simulator=\"iPad Pro\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier . --check",
    "format:fix": "prettier --write .",
    "start": "react-native start --reset-cache",
    "test": "jest",
    "prepare": "husky install"
  },
  "dependencies": {
    "@formatjs/intl-datetimeformat": "^6.10.0",
    "@formatjs/intl-displaynames": "^6.5.0",
    "@formatjs/intl-getcanonicallocales": "^2.2.1",
    "@formatjs/intl-listformat": "^7.4.0",
    "@formatjs/intl-locale": "^3.3.2",
    "@formatjs/intl-numberformat": "^8.7.0",
    "@formatjs/intl-pluralrules": "^5.2.4",
    "@formatjs/intl-relativetimeformat": "^11.2.4",
    "@hookform/resolvers": "^3.1.1",
    "@react-native-async-storage/async-storage": "^1.18.2",
    "@react-navigation/bottom-tabs": "^6.5.8",
    "@react-navigation/drawer": "^6.6.3",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/native-stack": "^6.9.13",
    "@reduxjs/toolkit": "^1.9.5",
    "@rneui/base": "^4.0.0-rc.7",
    "@rneui/themed": "^4.0.0-rc.7",
    "axios": "^1.4.0",
    "humps": "^2.0.1",
    "i18next": "^23.2.6",
    "react": "18.2.0",
    "react-hook-form": "^7.45.1",
    "react-i18next": "^13.0.1",
    "react-native": "0.72.0",
    "react-native-blob-util": "^0.18.3",
    "react-native-config": "^1.5.1",
    "react-native-device-info": "^10.7.0",
    "react-native-error-boundary": "^1.2.3",
    "react-native-fast-image": "^8.6.3",
    "react-native-gesture-handler": "^2.12.0",
    "react-native-localize": "^3.0.1",
    "react-native-logs": "^5.0.1",
    "react-native-reanimated": "^3.3.0",
    "react-native-safe-area-context": "^4.6.3",
    "react-native-screens": "^3.22.0",
    "react-native-size-matters": "^0.4.0",
    "react-native-vector-icons": "^9.2.0",
    "react-redux": "^8.1.1",
    "redux-persist": "^6.0.0",
    "yup": "^1.2.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.0.2",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.6",
    "@tsconfig/react-native": "^3.0.0",
    "@types/humps": "^2.0.3",
    "@types/metro-config": "^0.76.3",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "babel-plugin-tsconfig-paths-module-resolver": "^1.0.4",
    "eslint": "^8.19.0",
    "husky": "^8.0.3",
    "jest": "^29.2.1",
    "lint-staged": "^13.2.3",
    "metro-react-native-babel-preset": "0.76.5",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=16"
  }
}
